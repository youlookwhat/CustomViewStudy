<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jingbin.customview">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CustomTitleViewActivity" />
        <activity android:name=".activity.CustomImageViewActivity" />
        <activity android:name=".activity.CustomProgressBarActivity" />
        <activity android:name=".activity.CustomVolumControlBarActivity" />
        <activity android:name=".activity.CustomImgContainerActivity" />
        <activity android:name=".activity.DeepUnderstandAttrActivity" />
    </application>

</manifest>